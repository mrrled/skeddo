<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        xmlns:views="clr-namespace:newUI.Views.ScheduleView"
        x:Class="newUI.Views.ScheduleWindow.ScheduleWindow"
        Title="ScheduleWindow">
    
    <Grid Grid.Row="1" ColumnDefinitions="*, 250" Margin="10, 0, 10, 10">
            <ContentControl Grid.Column="0" Content="{Binding ScheduleView}">
                <ContentControl.ContentTemplate>
                    <DataTemplate>
                        <views:ScheduleView/> 
                    </DataTemplate>
                </ContentControl.ContentTemplate>
            </ContentControl>
    </Grid>
</Window>
